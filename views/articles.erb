<h1>News Aggregator</h1>
  <h3><a href="/articles/new">Submit new article</a></h3>
<ul>
  <% @file.each do |article| %>
    <% article_details = article.split(",") %>
    <li>
      <p>Title: <%= article_details[0] %></p>
      <p>URL: <a target="_blank" href="<%= article_details[1] %>"> <%= article_details[1] %></a></p>
      <p>Description: <%= article_details[2] %></p>
    </li>
  <% end %>
</ul>
